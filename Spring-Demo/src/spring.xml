<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">

<beans>
	<bean id="triangle" class="org.learnspring.tutorial6.Triangle" autowire="byName">
	</bean>
	
	<bean id="pointA" class="org.learnspring.tutorial6.Point">
		<property name="x" value="0"></property>
		<property name="y" value="0"></property>
	</bean>
	
	<bean id="pointB" class="org.learnspring.tutorial6.Point">
		<property name="x" value="0"></property>
		<property name="y" value="-20"></property>
	</bean>
	
	<bean id="pointC" class="org.learnspring.tutorial6.Point">
		<property name="x" value="20"></property>
		<property name="y" value="0"></property>
	</bean>
</beans>